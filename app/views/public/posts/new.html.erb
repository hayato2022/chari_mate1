<div class="container">
  <h4>お気に入りの景色を投稿</h4>
  <h4>撮影した場所をクリック！</h4>
  <%= form_with model: @posts, url: posts_path, method: :post do |f| %>
    <div class="row">
      <%= f.label :image, "・動画、画像を選択" %> </br>
      <%= f.file_field :image, accept: "image/*" %>
    </div>

    <div class="row">
      <%= f.label :body, "・本文" %> </br>
      <%= f.text_area :body %>
    </div>

    <!--タグの投稿機能-->

    <!--場所の投稿機能-->

  <% end %>
</div>
